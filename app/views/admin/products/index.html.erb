<h1>商品一覧</h1>

<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜き価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <% @products.each do |product| %>
        <td><%= product %></td>
        <td><%= product.name %></td>
        <td></td>
        <td></td>
        <td></td>
        <% end %>
      </tr>
  </tbody>
</table>